{
  "name": "ai-online-assistant-fake-news-detector",
  "version": "1.0.0",
  "description": "A tool for identifying fake news articles.",
  "scripts": {
    "install:nn": "cd nn_server && pip3 install -r requirements.txt",
    "install:classifierUtil": "cd nn_server/classifier_utilities && pip3 install -r requirements.txt",
    "install:coreNN": "cd nn_server/core_nn && pip3 install -r requirements.txt",
    "install:spellingNN": "cd nn_server/nn_spelling_input && pip3 install -r requirements.txt",
    "install:factNN": "cd nn_server/nn_fact_input && pip3 install -r requirements.txt",
    "install:db": "cd db_server && npm install && npm audit fix && npm update",
    "install:api": "cd api_server && npm install && npm audit fix && npm update",
    "postinstall": "run-s install:*",
    "test": "mocha --exit \"**/*.mocha.test.js\" --timeout 30000",
    "start": "cd api_server && npm start",
    "startP": "cd api_server && npm run \"startP\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/COS301-SE-2020/AI-Online-Assistant-Fake-News-Detector.git"
  },
  "author": "5Bits",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/COS301-SE-2020/AI-Online-Assistant-Fake-News-Detector/issues"
  },
  "homepage": "https://github.com/COS301-SE-2020/AI-Online-Assistant-Fake-News-Detector#readme",
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.1",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.11",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {}
}